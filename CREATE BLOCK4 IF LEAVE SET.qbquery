DELIMITER$
DROP PROCEDURE IF EXISTS BLOCK4$
CREATE PROCEDURE BLOCK4 ()
OUTTER_BLOCK : BEGIN
    DECLARE STATE_PROGRAM INTEGER;
    SET STATE_PROGRAM = 1;
INNER_BLOCK : BEGIN
    IF (STATE_PROGRAM=1) THEN
    LEAVE INNER_BLOCK;
  END IF;
  SELECT 'THIS STATEMENT PROCESSING BY INNER BLOCK';
END INNER_BLOCK;
SELECT 'END OF PROGRAM';
END OUTTER_BLOCK$
