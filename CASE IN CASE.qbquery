CASE
WHEN (SALE_VALUE > 200)THEN
  CALL FREE_SHIPPING(SALE_ID);
CASE CUSTOMER_STATUS
  WHEN 'PLATINUM' THEN
    CALL APPLY_DISCOUNT(SALE_ID, 20);
  WHEN 'GOLD' THEN
    CALL APPLY_DISCOUNT(SALE_ID, 15);
  WHEN 'SILVER' THEN
    CALL APPLY_DISCOUNT(SALE_ID, 10);
  WHEN 'BRONZE' THEN
    CALL APPLY_DISCOUNT(SALE_ID, 5);
  ELSE
    SET NOT_FOUND = 1;
END CASE;
ELSE
    SET NOT FOUND = 1;
END CASE;